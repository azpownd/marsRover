<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="roverDSL" nsURI="http://www.rover.mars/RoverDSL" nsPrefix="roverDSL">
  <eClassifiers xsi:type="ecore:EClass" name="Mission">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="missiontype" eType="#//MissionType"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="safetyproperty" eType="#//Safety"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="lakelist" unique="false"
        upperBound="-1" eType="#//Colors"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="beginsentence" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="border" eType="#//Colors"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="forwardspeed" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="reversespeed" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="turndirection" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="colorlist" unique="false"
        upperBound="-1" eType="#//Colors"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="terminationcondition" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="flashingcolor" eType="#//Colors"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="finalsentence" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="MissionType">
    <eLiterals name="AvoidColors" literal="AvoidColors"/>
    <eLiterals name="FindColors" value="1" literal="FindColors"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="Colors">
    <eLiterals name="White" literal="White"/>
    <eLiterals name="Red" value="1" literal="Red"/>
    <eLiterals name="Blue" value="2" literal="Blue"/>
    <eLiterals name="Yellow" value="3" literal="Yellow"/>
    <eLiterals name="Black" value="4" literal="Black"/>
    <eLiterals name="Orange" value="5" literal="Orange"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="Safety">
    <eLiterals name="On" literal="On"/>
    <eLiterals name="Off" value="1" literal="Off"/>
  </eClassifiers>
</ecore:EPackage>
